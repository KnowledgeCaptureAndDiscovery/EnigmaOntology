    <!-- https://w3id.org/enigma#isImagingDataAnalystOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#isImagingDataAnalystOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/enigma#isDataAnalystOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Project"/>
        <vann:example xml:lang="en">JohnSmith isImagingDataAnalystOf BipolarProject</vann:example>
        <rdfs:comment xml:lang="en">Describes what project someone is an imaging data analyst of</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#isImagingDataCollectorOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#isImagingDataCollectorOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/enigma#isDataCollectorOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Project"/>
        <vann:example xml:lang="en">JohnSmith isImagingDataCollectorOf BipolarProject</vann:example>
        <rdfs:comment xml:lang="en">Describes what project someone collected imaging data for</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#isInvestigatorOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#isInvestigatorOf">
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Cohort"/>
        <vann:example xml:lang="en">JohnSmith isInvestigatorOf BipolarCohort</vann:example>
        <rdfs:comment xml:lang="en">Describes the cohort that someone is investigator of</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#isMainPricipalInvestigatorOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#isMainPricipalInvestigatorOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/enigma#isPrincipalInvestigatorOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Cohort"/>
        <vann:example xml:lang="en">JohnSmith isMainPrincipalInvestigatorof Cohort1</vann:example>
        <rdfs:comment xml:lang="en">Describes which cohorts somone is a main PI of</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#isMinorPIOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#isMinorPIOf">
