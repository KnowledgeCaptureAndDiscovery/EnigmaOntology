    <!-- http://purl.org/vocab/vann/example -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/example"/>
    


    <!-- https://w3id.org/enigma#vann:example -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/enigma#vann:example"/>
    


