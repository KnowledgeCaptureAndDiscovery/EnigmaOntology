    <!-- https://w3id.org/enigma#hasResearchInterests -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/enigma#hasResearchInterests">
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
