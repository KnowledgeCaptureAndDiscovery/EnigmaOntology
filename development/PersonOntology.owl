     -->

    


    <!-- http://www.w3.org/2006/vcard/ns#hasAddress -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2006/vcard/ns#hasAddress">
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/vcard/ns#Address"/>
        <rdfs:comment xml:lang="en">To specify the components of the address for the object</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2006/vcard/ns"/>
        <rdfs:label xml:lang="en">has address</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#contributesTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#contributesTo">
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Project"/>
        <vann:example xml:lang="en">JohnSmith contributedTo BipolarProject</vann:example>
        <rdfs:comment xml:lang="en">Describes the projects that a person contributed to</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#hasDegree -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#hasDegree">
