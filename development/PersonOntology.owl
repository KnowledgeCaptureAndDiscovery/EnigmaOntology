    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#isGeneticDataCollectorOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/enigma#isDataCollectorOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Project"/>
        <vann:example xml:lang="en">JohnSmith isGeneticDataCollectorOf BipolarProject</vann:example>
        <rdfs:comment xml:lang="en">Describes what project someone collected genetic data for</rdfs:comment>
