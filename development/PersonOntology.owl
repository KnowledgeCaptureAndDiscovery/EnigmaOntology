        <rdfs:comment xml:lang="en">Describes what degree a person has</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#hasDepartment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#hasDepartment">
