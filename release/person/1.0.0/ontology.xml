        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">describes the contributors of a project</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#hasDegree -->


    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#hasDegree">
