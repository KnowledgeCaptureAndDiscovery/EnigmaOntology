    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/vocab/vann/example -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/example"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/enigma#contributesTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#contributesTo">
        <owl:inverseOf rdf:resource="https://w3id.org/enigma#hasContributor"/>
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Person"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Project"/>
        <vann:example rdf:datatype="http://www.w3.org/2001/XMLSchema#string">JohnSmith contributedTo BipolarProject</vann:example>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">describes the projects that a person contributed to</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/enigma#hasAffiliate -->


    <owl:ObjectProperty rdf:about="https://w3id.org/enigma#hasAffiliate">
        <owl:inverseOf rdf:resource="https://w3id.org/enigma#isAffiliatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/enigma#Organization"/>
        <rdfs:range rdf:resource="https://w3id.org/enigma#Person"/>
