                                                    rdf:first "Population-based"^^xsd:string ;
                                                    rdf:rest rdf:nil
                                                  ]
                                       ]
                           ] ;
                <http://purl.org/vocab/vann/example> "Project1 hasStudyDesign \"Case-control\""^^xsd:string ;
                rdfs:comment "describes the study design of the project"^^xsd:string .


