###  https://w3id.org/enigma#hasMOU
:hasMOU rdf:type owl:DatatypeProperty ;
        rdfs:domain :WorkingGroup ;
        rdfs:range xsd:string ;
        <http://purl.org/vocab/vann/example> "WorkingGroup1 hasMOU www.mouexample.com"^^xsd:string ;
        rdfs:comment "describes the MOU links of the working group"^^xsd:string .


###  https://w3id.org/enigma#hasPublishedPaper
:hasPublishedPaper rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain :WorkingGroup ;
                   rdfs:range xsd:string ;
                   <http://purl.org/vocab/vann/example> "WorkingGroup1 hasPublishedPaper www.publishedpaperexample.com"^^xsd:string ,
                                                        "WorkingGroup2 hasPublishedPaper \"Article Title\""^^xsd:string ;
                   rdfs:comment "describes the links to published papers or the paper title if the link does not exist for the working group"^^xsd:string .


