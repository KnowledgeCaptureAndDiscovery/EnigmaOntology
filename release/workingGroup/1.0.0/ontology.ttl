###  https://w3id.org/enigma#hasCohort
:hasCohort rdf:type owl:ObjectProperty ;
           owl:inverseOf :isCohortInWorkingGroup ;
           rdfs:domain :WorkingGroup ;
           rdfs:range :Cohort ;
           <http://purl.org/vocab/vann/example> "BipolarWorkingGroup hasCohort AmsterdamBipolarCohort --> Bipolar Working Group includes the Amsterdam Bipolar Cohort"^^xsd:string ;
           rdfs:comment "describes what cohorts are part of the working group"^^xsd:string .


###  https://w3id.org/enigma#hasProject
:hasProject rdf:type owl:ObjectProperty ;
            owl:inverseOf :isProjectOf ;
            rdfs:domain :WorkingGroup ;
            rdfs:range :Project ;
            <http://purl.org/vocab/vann/example> "BipolarWorkingGroup hasProject BipolarCorticolProject --> The Bipolar Working Group is working on the Bipolar Corticol Project"^^xsd:string ;
            rdfs:comment "describes what projects are worked on by a specific working group"^^xsd:string .


###  https://w3id.org/enigma#hasWGChair
:hasWGChair rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasWGMember ;
            owl:inverseOf :isChairOfWG ;
            rdfs:domain :WorkingGroup ;
            rdfs:range :Person ;
            <http://purl.org/vocab/vann/example> "SZWorkingGroup hasWGChair JohnSmith"^^xsd:string ;
            rdfs:comment "describes who the chairs of a working group are"^^xsd:string .


###  https://w3id.org/enigma#hasWGJuniorLead
:hasWGJuniorLead rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasWGMember ;
                 owl:inverseOf :isJuniorLeadOfWG ;
                 rdfs:domain :WorkingGroup ;
                 rdfs:range :Person ;
                 <http://purl.org/vocab/vann/example> "BipolarWorkingGroup hasJuniorLead JohnSmith"^^xsd:string ;
                 rdfs:comment "describes who the junior leads of a working group are"^^xsd:string .


###  https://w3id.org/enigma#hasWGMember
:hasWGMember rdf:type owl:ObjectProperty ;
             owl:inverseOf :isMemberOfWG ;
             rdfs:domain :WorkingGroup ;
             rdfs:range :Person ;
             <http://purl.org/vocab/vann/example> "BipolarWorkingGroup hasMember JohnSmith --> The bipolar working group has John Smith as a member"^^xsd:string ;
             rdfs:comment "describes who the members of the working group are"^^xsd:string .


###  https://w3id.org/enigma#hasWGSeniorLead
:hasWGSeniorLead rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasWGMember ;
                 owl:inverseOf :isSeniorLeadOfWG ;
                 rdfs:domain :WorkingGroup ;
                 rdfs:range :Person ;
                 <http://purl.org/vocab/vann/example> "BipolarWorkingGroup hasSeniorLead JohnSmith"^^xsd:string ;
                 rdfs:comment "describes who the senior leads of a working group are"^^xsd:string .


###  https://w3id.org/enigma#hasWGSpecialContributor
:hasWGSpecialContributor rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasWGMember ;
                         owl:inverseOf :isSpecialContributorOfWG ;
                         rdfs:domain :WorkingGroup ;
                         rdfs:range :Person ;
                         <http://purl.org/vocab/vann/example> "BipolarWorkingGroup hasSpecialContributor JohnSmith"^^xsd:string ;
                         rdfs:comment "describes who the special contributors of a working group are"^^xsd:string .


###  https://w3id.org/enigma#isChairOfWG
:isChairOfWG rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isMemberOfWG ;
             rdfs:domain :Person ;
             rdfs:range :WorkingGroup ;
             <http://purl.org/vocab/vann/example> "JohnSmith isChairOfWG SZWorkingGroup"^^xsd:string ;
             rdfs:comment "describes what WorkingGroup a person is chair of"^^xsd:string .


###  https://w3id.org/enigma#isCohortInWorkingGroup
:isCohortInWorkingGroup rdf:type owl:ObjectProperty ;
                        rdfs:domain :Cohort ;
                        rdfs:range :WorkingGroup ;
                        <http://purl.org/vocab/vann/example> "AmsterdamBipolarCohort isCohortIn BipolarWorkingGroup --> The Amsterdam Bipolar Cohort is part of the Bipolar Working Group"^^xsd:string ;
                        rdfs:comment "describes what working the cohort is a part of"^^xsd:string .


###  https://w3id.org/enigma#isJuniorLeadOfWG
:isJuniorLeadOfWG rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :isMemberOfWG ;
                  rdfs:domain :Person ;
                  rdfs:range :WorkingGroup ;
                  <http://purl.org/vocab/vann/example> "JohnSmith isJuniorLeadOf BipolarWorkingGroup"^^xsd:string ;
                  rdfs:comment "describes what working group someone is a junior lead of"^^xsd:string .


###  https://w3id.org/enigma#isMemberOfWG
:isMemberOfWG rdf:type owl:ObjectProperty ;
              rdfs:domain :Person ;
              rdfs:range :WorkingGroup ;
              <http://purl.org/vocab/vann/example> "JohnSmith isMemberOfWorkingGroup BipolarWorkingGroup --> John Smith is a member of the bipolar working group"^^xsd:string ;
              rdfs:comment "describes what working group a person is a member of"^^xsd:string .


###  https://w3id.org/enigma#isProjectOf
:isProjectOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Project ;
             rdfs:range :WorkingGroup ;
             <http://purl.org/vocab/vann/example> "BipolarCorticolProject isProjectOf BipolarWorkingGroup --> The Bipolar Corticol Project is a project of the Bipolar Working Group"^^xsd:string ;
             rdfs:comment "describes what working group a project is part of"^^xsd:string .


###  https://w3id.org/enigma#isSeniorLeadOfWG
:isSeniorLeadOfWG rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :isMemberOfWG ;
                  rdfs:domain :Person ;
                  rdfs:range :WorkingGroup ;
                  <http://purl.org/vocab/vann/example> "JohnSmith isSeniorLeadOf WorkingGroup"^^xsd:string ;
                  rdfs:comment "describes what working group the person is a senior lead of"^^xsd:string .


###  https://w3id.org/enigma#isSpecialContributorOfWG
:isSpecialContributorOfWG rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :isMemberOfWG ;
                          rdfs:domain :Person ;
                          rdfs:range :WorkingGroup ;
                          <http://purl.org/vocab/vann/example> "JohnSmith isSpecialContributorOf BipolarWorkingGroup"^^xsd:string ;
                          rdfs:comment "describes the working group that a person is a special contributor of"^^xsd:string .


