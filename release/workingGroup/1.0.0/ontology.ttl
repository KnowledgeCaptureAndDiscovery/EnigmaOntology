              rdfs:range xsd:date ;
              <http://purl.org/vocab/vann/example> "WorkingGroup1 hasStartDate 2004-09-13"^^xsd:string ;
              rdfs:comment "describes the start date of a working group"^^xsd:string .


###  https://w3id.org/enigma#hasSubmittedPaper
:hasSubmittedPaper rdf:type owl:DatatypeProperty ;
                   rdfs:domain :WorkingGroup ;
                   rdfs:range xsd:string ;
                   <http://purl.org/vocab/vann/example> "WorkingGroup1 hasSubmittedPaper \"Paper title\""^^xsd:string ;
                   rdfs:comment "describes the titles of the submitted papers of a working group"^^xsd:string .


###  https://w3id.org/enigma#hasWGBriefDescription
:hasWGBriefDescription rdf:type owl:DatatypeProperty ;
                       rdfs:domain :WorkingGroup ;
                       rdfs:range xsd:string ;
                       <http://purl.org/vocab/vann/example> "SZWG hasWGBriefDescription \"finds correlations between schizophrenia and other conditions\""^^xsd:string ;
                       rdfs:comment "provides a brief description of the working group"^^xsd:string .


