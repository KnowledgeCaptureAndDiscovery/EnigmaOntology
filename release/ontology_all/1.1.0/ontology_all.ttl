      rdfs:label "has ethnic group"@en ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "African American" ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "Asian" ;
                                    rdf:rest
                                            [ a       rdf:List ;
                                              rdf:first "European" ;
                                              rdf:rest
                                                      [ a       rdf:List ;
                                                        rdf:first "Latino" ;
                                                        rdf:rest
                                                                [ a       rdf:List ;
                                                                  rdf:first "Two or More Races" ;
                                                                  rdf:rest ()
                                                                ]
                                                      ]
                                            ]
                                  ]
                        ]
              ] ;
      vann:example "Cohort1 hasEthnicGroup \"European\""@en .

