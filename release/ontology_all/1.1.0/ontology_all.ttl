@prefix :        <https://w3id.org/enigma#> .
@prefix vaem:    <http://www.linkedmodel.org/schema/vaem#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix terms:   <http://purl.org/dc/terms/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann1:   <https://w3id.org/enigma#vann:> .
@prefix vann:    <http://purl.org/vocab/vann/> .

:rationale
      a       owl:AnnotationProperty .

<http://www.w3.org/2006/vcard/ns#locality>
      a       owl:DatatypeProperty ;
      rdfs:comment "The locality (e.g. city or town) associated with the address of the object"@en ;
      rdfs:domain <http://www.w3.org/2006/vcard/ns#Address> ;
      rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
      rdfs:range xsd:string .

:hasNumberOfT1w
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the amount of T1-weighted image data collected by a project cohort"@en ;
      rdfs:domain :ProjectCohort ;
      rdfs:label "has number of T1w"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example "ProjectCohortSZ hasNumberOfT1w 8"@en .

:hasNumberOfT2
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the amount of T2 image data  collected by a project cohort"@en ;
      rdfs:domain :ProjectCohort ;
      rdfs:label "has number of T2"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example "ProjectCohortSZ hasNumberOfT2 10"@en .

:hasWorkingGroupMember
      a       owl:ObjectProperty ;
      rdfs:comment "Describes who the members of the working group are"@en , "Describes what working group a person is a member of"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "has working group member"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/member> ;
      vann:example "BipolarWorkingGroup hasWorkingGroupMember JohnSmith"@en .

vaem:rationale
      a       owl:AnnotationProperty .

<http://www.w3.org/2006/vcard/ns#Address>
      a       owl:Class ;
      rdfs:comment "To specify the components of the delivery address for the object"@en ;
      rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
      rdfs:label "Address"@en .

<http://www.w3.org/2006/vcard/ns#street-address>
      a       owl:DatatypeProperty ;
      rdfs:comment "The street address associated with the address of the object"@en ;
      rdfs:domain <http://www.w3.org/2006/vcard/ns#Address> ;
      rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
      rdfs:range xsd:string .

:hasExclusionCriteria
      a       owl:DatatypeProperty ;
      rdfs:comment "Criteria used to exclude cohort patients"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :CohortGroup)
              ] ;
      rdfs:label "has exclusion criteria"@en ;
      rdfs:range xsd:string .

:usedSoftware
      a       owl:ObjectProperty ;
      rdfs:comment "Property to identify the software used by a cohort"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :CohortGroup)
              ] ;
      rdfs:label "used software"@en ;
      rdfs:range :Software .

:contributor
      a       owl:ObjectProperty ;
      rdfs:comment "Property that indicates who is a contributor to a project or cohort"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:AcademicArticle :Cohort :Project)
              ] ;
      rdfs:label "contributor"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf terms:contributor ;
      vaem:rationale "Property added to restrict the domain to projects, cohorts and academic articles. This is done for facilitating the interaction with the wiki interface"@en .

<https://w3id.org/enigma#vann:example>
      a       owl:AnnotationProperty .

:hasReviewer
      a       owl:ObjectProperty ;
      rdfs:comment "Property that indicates who contributed to the paper reviewing the initial drafts and providing comments"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:AcademicArticle :Project)
              ] ;
      rdfs:label "has reviewer"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :contributor ;
      vann:example "BipolarProject hasReviewer JohnSmith"@en .

:RespiratoryDisorder
      a       owl:Class ;
      rdfs:comment "Disorders which affect the respiratory system"@en ;
      rdfs:label "Respiratory Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "Asthma"@en .

<http://xmlns.com/foaf/0.1/Person>
      a       owl:Class ;
      rdfs:isDefinedBy "http://xmlns.com/foaf/0.1/"@en ;
      owl:equivalentClass :Person .

:Protocol
      a       owl:Class ;
      rdfs:comment "An image processing or genetic analysis protocol used to extract brain feature data"@en ;
      rdfs:label "Protocol"@en ;
      vann:example "ENIGMA-Sulci Protocol"@en .

:hasMainPrincipalInvestigator
      a       owl:ObjectProperty ;
      rdfs:comment "Describes which cohorts somone is a main PI of"@en ;
      rdfs:domain :Cohort ;
      rdfs:label "has main principal investigator"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :hasPrincipalInvestigator ;
      vann:example "Cohort1 hasMainPrincipalInvestigatorof JohnSmith"@en .

:Manufacturer
      a       owl:Class ;
      rdfs:comment "Manufacturer of the scanner"@en ;
      rdfs:label "Manufacturer"@en ;
      vann:example "Siemens"@en .

:usesScanner
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the scanner used by the cohort"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :CohortGroup)
              ] ;
      rdfs:label "uses scanner"@en ;
      rdfs:range xsd:string ;
      vann:example "SZCohort usesScanner \"1.5T-GE-Signa\""@en .

:hasStudyFeature
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the study features of a cohort group (Clinical trial; Twin/Family study; Logitudinal; in-vivo; ex-vivo; post-mortem)"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has study feature"@en ;
      rdfs:range xsd:string ;
      vann:example "CohortGroup1 hasStudyFeatures \"Clinical trial, Twin/Family study\""@en .

:givenName
      a       owl:DatatypeProperty ;
      rdfs:comment "The given name of some person."@en ;
      rdfs:domain :Person ;
      rdfs:label "given name"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/givenName> ;
      vaem:rationale "Property added to restrict the domain to enigma:Person. This is done for facilitating the interaction with the wiki interface"@en .

:hasPercentSelectLoci
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the percent of cohort group biosamples which are from select loci (some SNPs/CNVs)"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has percent select loci"@en ;
      rdfs:range xsd:double ;
      vann:example "CohortGroupSZ hasPercentSelectLoci 67.0"@en .

:hasCategory
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes a categorical representation of the broad kind of data that a protocol is used for"@en ;
      rdfs:domain :Protocol ;
      rdfs:label "has category"@en ;
      rdfs:range xsd:string ;
      vann:example "ENIGMA-SulciProtocol hasCategory \"Imaging\""@en .

:ScannerSoftware
      a       owl:Class ;
      rdfs:comment "The software associated with a given scanner"@en ;
      rdfs:label "ScannerSoftware"@en ;
      vann:example "FreeSurfer5.1.0"@en .

:hasWorkingGroupChair
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the working groups that someone is a chair of"@en , "Describes who the chairs of a working group are"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "has working group chair"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :hasWorkingGroupMember ;
      vann:example "BipolarWorkingGroup hasWorkingGroupChair JohnSmith"@en , "SZWorkingGroup hasWorkingGroupChair JohnSmith"@en .

:hasVersion
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the version of the protocol"@en ;
      rdfs:domain :Protocol ;
      rdfs:label "has version"@en ;
      rdfs:range xsd:string ;
      vann:example "ENIGMA Cortical Quality Control Protocol hasVersion \"2.0 April 2017\""@en .

<http://xmlns.com/foaf/0.1/member>
      a       owl:ObjectProperty ;
      rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/member> , <http://xmlns.com/foaf/0.1/> .

:hasDataAcquisitionMatrix
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the size of the data acquisition matrix used"@en ;
      rdfs:domain :AcquisitionProtocol ;
      rdfs:range xsd:string ;
      vann:example "1.5T-GE-Signa hasDataAcquisitionMatrix \"256x256\" --> The size of the data acquisition matrix is 256x256"@en .

:hasPercentOfMale
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the percent of males in the cohort group"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has percent of male"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example "CohortGroup1 hasNumberOfMale 50"@en .

:Condition
      a       owl:Class ;
      rdfs:comment "A state or mood experienced by the members of a cohort"@en ;
      rdfs:label "Condition"@en ;
      vann:example "Euthymia"@en .

:hasPublishedPaper
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the links to published papers or the paper title (if link does not exist) for the working group"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "has published paper"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf owl:topDataProperty ;
      vann:example "WorkingGroup2 hasPublishedPaper \"Article Title\""@en , "WorkingGroup1 hasPublishedPaper www.publishedpaperexample.com"@en .

vann:preferredNamespaceUri
      a       owl:AnnotationProperty .

:hasStudyDesign
      a       owl:DatatypeProperty ;
      rdfs:comment """Describes the study design of the cohort group or project (Case/control; Population; Healthy only; Patients only)

NOTE: study design options are mutually exclusive for Cohort Groups but not Projects"""@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:CohortGroup :Project)
              ] ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:label "has study design"@en ;
      rdfs:range xsd:string ;
      vann:example "Project1 hasStudyDesign \"Case/control, Population\""@en , "CohortGroup1 hasStudyDesign \"Population\""@en .

:hasGeneticDataType
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the genetic data types that a cohort or project has"@en , "Describes the genetic data types that were collected for a cohort or project"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:label "has genetic data type"@en ;
      rdfs:range :GeneticDataType ;
      rdfs:subPropertyOf :hasDataType ;
      vann:example "BipolarProject hasGeneticDataType Exome"@en , "Cohort1 hasGeneticDataType epigenetic"@en .

:MentalHealthDisorder
      a       owl:Class ;
      rdfs:comment "Disorders which can affect mood, behavior, and thinking"@en ;
      rdfs:label "Mental Health Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "OCD"@en .

:NeurologicalDisorder
      a       owl:Class ;
      rdfs:comment "Disorders which affect the nervous system"@en ;
      rdfs:label "Neurological Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "Epilepsy"@en .

:SubstanceAbuse
      a       owl:Class ;
      rdfs:comment "A mental health disorder defined by the overusage or abuse of a substance"@en ;
      rdfs:label "Substance Abuse"@en ;
      rdfs:subClassOf :MentalHealthDisorder ;
      vann:example "Alcoholism"@en .

:hasAcquisitionDirection
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the acquisition direction of the scanner"@en ;
      rdfs:domain :AcquisitionProtocol ;
      rdfs:range :AcquisitionDirection ;
      vann:example "1.5T-GE-Signa hasAcquisitionDirection coronal --> The acquisition direction of this 1.5T-GE-Signa is coronal"@en .

:hasFieldStrength
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the field strength of the cohort"@en ;
      rdfs:domain :Scanner ;
      rdfs:range xsd:double ;
      vann:example "1.5T-GE-Signa hasFieldStrength 1.5 --> The field strength of 1.5T-GE-Signa is 1.5"@en .

:hasGeneticDataCollector
      a       owl:ObjectProperty ;
      rdfs:comment "Describes what project someone collected genetic data for"@en ;
      rdfs:domain :Project ;
      rdfs:label "has genetic data collector"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :hasDataCollector ;
      vann:example "BipolarProject hasGeneticDataCollector JohnSmith"@en .

terms:spatial
      a       owl:DatatypeProperty ;
      rdfs:isDefinedBy terms:spatial .

<http://vivoweb.org/ontology/core#College>
      a       owl:Class ;
      rdfs:comment "A college contributing to a given ENIGMA project"@en ;
      rdfs:isDefinedBy <http://vivoweb.org/ontology/core> ;
      rdfs:label "College"@en ;
      rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> ;
      vann:example "Los Angeles City College"@en .

:forOrganization
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the organization associated with the qualified affiliation"@en ;
      rdfs:domain :QualifiedAffiliation ;
      rdfs:label "for organization"@en ;
      rdfs:range <http://xmlns.com/foaf/0.1/Organization> ;
      vann:example "JohnSmith hasQualifiedAffiliation \"QualifiedAffiliation\" forOrganization USC"@en .

:AcquisitionProtocol
      a       owl:Class ;
      rdfs:comment "The protocol used to acquire the scanner data types"@en ;
      rdfs:label "AcquisitionProtocol"@en ;
      vann:example "ENIGMA-Sulci"@en .

<http://schema.org/distribution>
      a       owl:ObjectProperty ;
      rdfs:domain <http://schema.org/Dataset> ;
      rdfs:isDefinedBy <http://schema.org> ;
      rdfs:range <http://schema.org/DataDownload> .

:hasProjectLead
      a       owl:ObjectProperty ;
      rdfs:comment "Describes what project someone is a project lead of"@en , "Describes who the leads of a project are"@en ;
      rdfs:domain :Project ;
      rdfs:label "has project lead"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf terms:contributor ;
      vann:example "BipolarProject hasProjectLead JohnSmith"@en .

:GeneticAssociationProtocol
      a       owl:Class ;
      rdfs:comment "ENIGMA Protocols for genetic associations"@en ;
      rdfs:label "Genetic Association Protocol"@en ;
      vann:example "ENIGMA3 – GWAS Meta Analysis of Cortical Thickness and Surface Area - Association Protocol"@en .

:hasNumberOfEEG
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the amount of electroencephalography image data collected by a project cohort"@en ;
      rdfs:domain :ProjectCohort ;
      rdfs:label "has number of EEG"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example "ProjectCohortSZ hasNumberOfEEG 30"@en .

:DataType
      a       owl:Class ;
      rdfs:comment "Specifies the kinds of data collected"@en ;
      rdfs:label "Data Type"@en , "DataType"@en .

<http://xmlns.com/foaf/0.1/Organization>
      a       owl:Class ;
      rdfs:comment "An organization contributing to a given ENIGMA project"@en ;
      rdfs:isDefinedBy <http://xmlns.com/foaf/0.1> ;
      owl:equivalentClass :Organization .

:currentJobTitle
      a       owl:DatatypeProperty ;
      rdfs:comment "Data property that aims to capture the current job title of a person. Examples are student, faculty, staff, postdoc, etc."@en ;
      rdfs:domain :Person ;
      rdfs:label "current job title"@en ;
      rdfs:range xsd:string ;
      vann:example "JohnSmith currentJobTitle \"Postdoc\""@en .

:OrganicMedicalDisorder
      a       owl:Class ;
      rdfs:comment "Disorder characterized by decreased mental function due to a medical or physical disease"@en ;
      rdfs:label "Organic Medical Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "Delirium"@en .

:hasPercentMitochondrialGenotyping
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the percent of cohort group biosamples which are from mitochondrial genotyping"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has percent mitochondrial genotyping"@en ;
      rdfs:range xsd:double ;
      vann:example "CohortGroupSZ hasPercentMitochondrialGenotyping 44.5"@en .

:hasMiddleInitial
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes middle initial of a person" ;
      rdfs:domain :Person ;
      rdfs:label "has middle initial"@en ;
      rdfs:range xsd:string ;
      vann:example "John A Smith hasMiddleInitial \"A\"" .

:shortName
      a       owl:DatatypeProperty ;
      rdfs:comment "A short name for the working group, in case the working group name is too long"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "short name"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf terms:title ;
      vann:example "SchizophreniaWG shortName SZWG"@en .

vann:preferredNamespacePrefix
      a       owl:AnnotationProperty .

terms:contributor
      a       owl:AnnotationProperty , owl:ObjectProperty ;
      rdfs:domain :Project ;
      rdfs:isDefinedBy terms:contributor ;
      rdfs:range :Person .

:hasSpecies
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the species of the subjects of a cohort (Humans, Chimps, Mice, etc.)"@en ;
      rdfs:domain :Cohort ;
      rdfs:label "has species"@en ;
      rdfs:range xsd:string ;
      vann:example "Cohort1 hasSpecies \"humans\""@en .

terms:identifier
      a       owl:DatatypeProperty ;
      rdfs:isDefinedBy terms:identifier .

:BrainScanDataType
      a       owl:Class ;
      rdfs:comment "Specifies the kinds of brain scan data collected"@en ;
      rdfs:label "Brain Scan Data Type"@en ;
      rdfs:subClassOf :DataType ;
      vann:example "EEG"@en .

:hasDataset
      a       owl:ObjectProperty ;
      rdfs:comment "Property that links a project cohort with the data being described."@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :ProjectCohort)
              ] ;
      rdfs:label "has dataset"@en ;
      rdfs:range :Dataset .

:usedBodyCoil
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes if a body coil was used to scan by the scanner"@en ;
      rdfs:domain :AcquisitionProtocol ;
      rdfs:range xsd:boolean ;
      vann:example "1.5T-GE-Signa usedBodyCoil true --> This 1.5T-GE-Signa scanner used a body coil to scan"@en .

:usesProtocol
      a       owl:ObjectProperty ;
      rdfs:comment "Describes which data processing protocol was used in a project"@en ;
      rdfs:domain :Project ;
      rdfs:label "uses protocol"@en ;
      rdfs:range :Protocol ;
      vann:example "BipolarProject usesProtocol ENIGMA-SulciProtocol"@en .

:ImageAcquisitionProtocol
      a       owl:Class ;
      rdfs:comment "Procedure used to collect image data within the cohort"@en ;
      rdfs:label "Image Acquisition Protocol"@en ;
      vann:example "ENIGMA-Sulci"@en .

xsd:date
      a       rdfs:Datatype .

:hasPregnantMembers
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes if the cohort has pregnant members"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has pregnant members"@en ;
      rdfs:range xsd:boolean ;
      vann:example "CohortGroup1 hasPregnantMembers false"@en .

:hasEndDate
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the end date of the described resource (a working group, a person in an organizaiton, etc.)"@en ;
      rdfs:domain owl:Thing ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:QualifiedAffiliation :WorkingGroup)
              ] ;
      rdfs:label "has end date"@en ;
      rdfs:range xsd:date ;
      vann:example "BipolarProject hasStartDate 2018-06-23"@en , "JohnSmith hasQualifiedAffiliation \"QualifiedAffiliation\" hasEndDate \"2018-07-13\""@en .

<http://schema.org/name>
      a       owl:DatatypeProperty ;
      rdfs:isDefinedBy <http://schema.org> .

:hasNumberOfTransgenderMtoF
      a       owl:DatatypeProperty ;
      rdfs:comment "Describing the number of male to female transgender individuals in a cohort group"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has number of transgender m to f"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example "CohortGroup1 hasNumberOfTransgenderMtoF 20"@en .

:familyName
      a       owl:DatatypeProperty ;
      rdfs:comment "The family name of some person."@en ;
      rdfs:domain :Person ;
      rdfs:label "family name"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/familyName> ;
      vaem:rationale "Property added to restrict the domain to enigma:Person. This is done for facilitating the interaction with the wiki interface"@en .

<http://vivoweb.org/ontology/core#University>
      a       owl:Class ;
      rdfs:comment "A university contributing to a given ENIGMA project"@en ;
      rdfs:isDefinedBy <http://vivoweb.org/ontology/core> ;
      rdfs:label "University"@en ;
      rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> ;
      vann:example "USC"@en .

:Dataset
      a       owl:Class ;
      rdfs:comment "A dataset containing information about an ENIGMA project cohort"@en ;
      rdfs:label "Dataset"@en ;
      rdfs:subClassOf <http://schema.org/Dataset> .

:AcademicArticle
      a       owl:Class ;
      rdfs:comment "An academic article, submitted as a result of the work in an ENIGMA project"@en ;
      rdfs:label "Academic article"@en .

:hasContact
      a       owl:ObjectProperty ;
      rdfs:comment "Property that points to the person that is a contact for the project or cohort"@en , "Describes who to contact for inquiries about the protocol"@en ;
      rdfs:domain :Protocol ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:label "has contact"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      vann:example "ENIGMA-SulciProtocol hasContact JohnSmith"@en , "BipolarCohort hasContact JohnSmith"@en .

:hasEmail
      a       owl:ObjectProperty ;
      rdfs:comment "Property that indicates which is the email of a person"@en ;
      rdfs:domain :Person ;
      rdfs:label "has email"@en ;
      rdfs:range owl:Thing ;
      rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/mbox> ;
      vaem:rationale "Property added to facilitate interaction with domain and ranges in the wiki. It's equivalent to foaf:mbox"@en .

:isScannerOf
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the protocols that the scanner is used for"@en ;
      rdfs:domain :Scanner ;
      rdfs:range :AcquisitionProtocol ;
      vann:example "1.5T-GE-Siemens isScannerOf AcquisitionProtocol1"@en .

:hasInvestigator
      a       owl:ObjectProperty ;
      rdfs:comment "Describe the investigators of a cohort"@en , "Describes the cohort that someone is investigator of"@en ;
      rdfs:domain :Cohort ;
      rdfs:label "has investigator"@en ;
      rdfs:range :Person ;
      vann:example "BipolarCohort hasInvestigator JohnSmith"@en .

:AcquisitionDirection
      a       owl:Class ;
      rdfs:comment "The acquisition direction of a scanner"@en ;
      rdfs:label "AcquisitionDirection"@en ;
      vann:example "Axial"@en .

:GenotypeImputationProtocol
      a       owl:Class ;
      rdfs:comment "ENIGMA Protocols for genetic imputations"@en ;
      rdfs:label "Genotype Imputation Protocol"@en ;
      vann:example "ENIGMA3 – GWAS Meta Analysis of Cortical Thickness and Surface Area - Imputation Protocol"@en .

:isScannerModel
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes what the scanner model is"@en ;
      rdfs:domain :Scanner ;
      rdfs:range xsd:string ;
      vann:example "1.5T-GE-Signa isScannerModel \"Signa\" --> The scanner model for 1.5T-GE-Signa is Signa"@en .

:ConflictOfInterest
      a       owl:Class ;
      rdfs:comment "Concept used to represent a conflict of interest of an author in a paper"@en ;
