      rdfs:label "Image Acquisition Protocol"@en ;
      vann:example "ENIGMA-Sulci"@en .

xsd:date
      a       rdfs:Datatype .

:hasPregnantMembers
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes if the cohort has pregnant members"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has pregnant members"@en ;
      rdfs:range xsd:boolean ;
      vann:example "CohortGroup1 hasPregnantMembers false"@en .

:hasEndDate
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the end date of the described resource (a working group, a person in an organizaiton, etc.)"@en ;
