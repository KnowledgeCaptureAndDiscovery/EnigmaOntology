      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:label "has contact"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf owl:topObjectProperty ;
