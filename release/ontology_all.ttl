@prefix :        <https://w3id.org/enigma#> .
@prefix vaem:    <http://www.linkedmodel.org/schema/vaem#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix terms:   <http://purl.org/dc/terms/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann1:   <https://w3id.org/enigma#vann:> .
@prefix vann:    <http://purl.org/vocab/vann/> .

:hasFirstDegreeRelativeWithDisorder
      a       owl:ObjectProperty ;
      rdfs:comment "Describes what diseases the first degree relatives of the members of the cohort group have"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has first degree relative with disorder"@en ;
      rdfs:range :Disorder ;
      rdfs:subPropertyOf :hasFamilyHistoryOf ;
      vann:example "CohortGroup1 hasFirstDegreeRelativeWithDisorder Psychosis --> CohortGroup1 only includes people who have a first degree relative with psychosis"@en .

:rationale
      a       owl:AnnotationProperty .

<http://www.w3.org/2006/vcard/ns#locality>
      a       owl:DatatypeProperty ;
      rdfs:comment "The locality (e.g. city or town) associated with the address of the object"@en ;
      rdfs:domain <http://www.w3.org/2006/vcard/ns#Address> ;
      rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
      rdfs:range xsd:string .

:SpecificDisorderInfo
      a       owl:Class ;
      rdfs:comment "Details unique to a given disorder of the cohort"@en ;
      rdfs:label "Specific Disorder Info"@en ;
      vann:example "Severity of a disorder"@en .

:doesNotHaveIntellectualDisability
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes whether a cohort excludes people with an intellectual disability"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "does not have intellectual disability"@en ;
      rdfs:range xsd:boolean ;
      vann:example "Cohort1 doesNotHaveIntellectualDisability true"@en .

:hasWorkingGroupMember
      a       owl:ObjectProperty ;
      rdfs:comment "Describes who the members of the working group are"@en , "Describes what working group a person is a member of"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "has working group member"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/member> ;
      vann:example "BipolarWorkingGroup hasWorkingGroupMember JohnSmith"@en .

vaem:rationale
      a       owl:AnnotationProperty .

<http://www.w3.org/2006/vcard/ns#Address>
      a       owl:Class ;
      rdfs:comment "To specify the components of the delivery address for the object"@en ;
      rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
      rdfs:label "Address"@en .

:includesMale
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes whether a cohort includes male participants"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :CohortGroup :ProjectCohort)
              ] ;
      rdfs:label "includes male"@en ;
      rdfs:range xsd:boolean ;
      vann:example "Cohort1 includesMale true"@en .

<http://www.w3.org/2006/vcard/ns#street-address>
      a       owl:DatatypeProperty ;
      rdfs:comment "The street address associated with the address of the object"@en ;
      rdfs:domain <http://www.w3.org/2006/vcard/ns#Address> ;
      rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
      rdfs:range xsd:string .

:contributor
      a       owl:ObjectProperty ;
      rdfs:comment "Property that indicates who is a contributor to a project or cohort"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:AcademicArticle :Cohort :Project)
              ] ;
      rdfs:label "contributor"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf terms:contributor ;
      vaem:rationale "Property added to restrict the domain to projects, cohorts and academic articles. This is done for facilitating the interaction with the wiki interface"@en .

<https://w3id.org/enigma#vann:example>
      a       owl:AnnotationProperty .

:hasReviewer
      a       owl:ObjectProperty ;
      rdfs:comment "Property that indicates who contributed to the paper reviewing the initial drafts and providing comments"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:AcademicArticle :Project)
              ] ;
      rdfs:label "has reviewer"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :contributor .

:RespiratoryDisorder
      a       owl:Class ;
      rdfs:comment "Disorders which affect the respiratory system"@en ;
      rdfs:label "Respiratory Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "Asthma"@en .

<http://xmlns.com/foaf/0.1/Person>
      a       owl:Class ;
      rdfs:isDefinedBy "http://xmlns.com/foaf/0.1/"@en ;
      owl:equivalentClass :Person .

:hasMaxAge
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes inclusion criteria for maximum age of participants in a CohortGroup"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has max age"@en ;
      rdfs:range xsd:positiveInteger ;
      vann:example "CohortGroup1 hasMaxAge 60"@en .

:withMaxDurationOfDisorder
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the maximum duration (in years) of a disease"@en ;
      rdfs:domain :SpecificDisorderInfo ;
      rdfs:label "with max duration of disorder"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example """CohortGroup1 hasDisorderDetails Schizophrenia
Schizophrenia withMaxDurationOfDisorder 20

--> CohortGroup1 only includes people who had bipolar disorder for at most 20 years"""@en .

:hasMainPrincipalInvestigator
      a       owl:ObjectProperty ;
      rdfs:comment "Describes which cohorts somone is a main PI of"@en ;
      rdfs:domain :Cohort ;
      rdfs:label "has main principal investigator"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :hasPrincipalInvestigator ;
      vann:example "Cohort1 hasMainPrincipalInvestigatorof JohnSmith"@en .

:Manufacturer
      a       owl:Class ;
      rdfs:comment "Manufacturer of the scanner"@en ;
      rdfs:label "Manufacturer"@en ;
      vann:example "Siemens"@en .

:hasFamilyHistoryOf
      a       owl:ObjectProperty ;
      rdfs:comment "Describes what diseases the members of the cohort group have a family history of"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has family history of"@en ;
      rdfs:range :Disorder ;
      vann:example "CohortGroup1 hasFamilyHistoryOf CardiovascularDisease --> CohortGroup1 only includes people who have a family history of cardiovascular diseases"@en .

:givenName
      a       owl:DatatypeProperty ;
      rdfs:comment "The given name of some person."@en ;
      rdfs:domain :Person ;
      rdfs:label "given name"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/givenName> ;
      vaem:rationale "Property added to restrict the domain to enigma:Person. This is done for facilitating the interaction with the wiki interface"@en .

:ScannerSoftware
      a       owl:Class ;
      rdfs:comment "The software associated with a given scanner"@en ;
      rdfs:label "ScannerSoftware"@en ;
      vann:example "FreeSurfer5.1.0"@en .

:hasWorkingGroupChair
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the working groups that someone is a chair of"@en , "Describes who the chairs of a working group are"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "has working group chair"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :hasWorkingGroupMember ;
      vann:example "BipolarWorkingGroup hasWorkingGroupChair JohnSmith"@en , "SZWorkingGroup hasWorkingGroupChair JohnSmith"@en .

<http://xmlns.com/foaf/0.1/member>
      a       owl:ObjectProperty ;
      rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/member> , <http://xmlns.com/foaf/0.1/> .

:hasDisorderDetails
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the details of a particular disorder that a cohort group has"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "has disorder details"@en ;
      rdfs:range :SpecificDisorderInfo ;
      vann:example "CohortGroup1 hasDisorderDetails SZ20938"@en .

:hasDataAcquisitionMatrix
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the size of the data acquisition matrix used"@en ;
      rdfs:domain :AcquisitionProtocol ;
      rdfs:range xsd:string ;
      vann:example "1.5T-GE-Signa hasDataAcquisitionMatrix \"256x256\" --> The size of the data acquisition matrix is 256x256"@en .

:withMinDurationOfDisorder
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the minimum duration (in years) of a disease"@en ;
      rdfs:domain :SpecificDisorderInfo ;
      rdfs:label "with min duration of disorder"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example """CohortGroup1 hasDisorderDetails Schizophrenia
Schizophrenia withMinDurationOfDisorder 10

--> CohortGroup1 only includes people had bipolar disorder for at least 10 years"""@en .

:Condition
      a       owl:Class ;
      rdfs:comment "A state or mood experienced by the members of a cohort"@en ;
      rdfs:label "Condition"@en ;
      vann:example "Euthymia"@en .

:hasPublishedPaper
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the links to published papers or the paper title (if link does not exist) for the working group"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "has published paper"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf owl:topDataProperty ;
      vann:example "WorkingGroup2 hasPublishedPaper \"Article Title\""@en , "WorkingGroup1 hasPublishedPaper www.publishedpaperexample.com"@en .

vann:preferredNamespaceUri
      a       owl:AnnotationProperty .

:hasLongitudinalTimepoint
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the number of times a patient has been examined across time"@en ;
      rdfs:domain :Cohort ;
      rdfs:label "has longitudinal timepoint"@en ;
      rdfs:range xsd:nonNegativeInteger ;
      vann:example "BipolarCorticolCohort hasLongitudinalTimepoint 3"@en .

:hasStudyDesign
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the study design of the cohort or project"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:label "has study design"@en ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "Case-control" ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "Population-based" ;
                                    rdf:rest ()
                                  ]
                        ]
              ] ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:oneOf
                        [ a       rdf:List ;
                          rdf:first "Case-Control" ;
                          rdf:rest
                                  [ a       rdf:List ;
                                    rdf:first "Population-Based" ;
                                    rdf:rest ()
                                  ]
                        ]
              ] ;
      vann:example "Cohort1 hasStudyDesign Case-Control"@en , "Project1 hasStudyDesign \"Case-control\""@en .

:hasGeneticDataType
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the genetic data types that a cohort or project has"@en , "Describes the genetic data types that were collected for a cohort or project"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (:Cohort :Project)
              ] ;
      rdfs:label "has genetic data type"@en ;
      rdfs:range :GeneticDataType ;
      rdfs:subPropertyOf :hasDataType ;
      vann:example "BipolarProject hasGeneticDataType Exome"@en , "Cohort1 hasGeneticDataType epigenetic"@en .

:MentalHealthDisorder
      a       owl:Class ;
      rdfs:comment "Disorders which can affect mood, behavior, and thinking"@en ;
      rdfs:label "Mental Health Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "OCD"@en .

:doesNotHaveFirstDegreeRelativeWithDisorder
      a       owl:ObjectProperty ;
      rdfs:comment "Describes which diseases the first degree relatives of cohort group members do NOT have"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "does not first degree relative with disorder"@en ;
      rdfs:range :Disorder ;
      rdfs:subPropertyOf :doesNotHaveFamilyHistoryOf ;
      vann:example "CohortGroup1 doesNotHaveFirstDegreeRelativeWithDisorder BipolarDisorder --> CohortGroup1 excludes people who have a first degree relative with bipolar disorder"@en ;
      owl:propertyDisjointWith
              :hasFirstDegreeRelativeWithDisorder .

:NeurologicalDisorder
      a       owl:Class ;
      rdfs:comment "Disorders which affect the nervous system"@en ;
      rdfs:label "Neurological Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "Epilepsy"@en .

:SubstanceAbuse
      a       owl:Class ;
      rdfs:comment "A mental health disorder defined by the overusage or abuse of a substance"@en ;
      rdfs:label "Substance Abuse"@en ;
      rdfs:subClassOf :MentalHealthDisorder ;
      vann:example "Alcoholism"@en .

:hasAcquisitionDirection
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the acquisition direction of the scanner"@en ;
      rdfs:domain :AcquisitionProtocol ;
      rdfs:range :AcquisitionDirection ;
      vann:example "1.5T-GE-Signa hasAcquisitionDirection coronal --> The acquisition direction of this 1.5T-GE-Signa is coronal"@en .

:hasFieldStrength
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes the field strength of the cohort"@en ;
      rdfs:domain :Scanner ;
      rdfs:range xsd:double ;
      vann:example "1.5T-GE-Signa hasFieldStrength 1.5 --> The field strength of 1.5T-GE-Signa is 1.5"@en .

:hasGeneticDataCollector
      a       owl:ObjectProperty ;
      rdfs:comment "Describes what project someone collected genetic data for"@en ;
      rdfs:domain :Cohort ;
      rdfs:label "has genetic data collector"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf :hasDataCollector ;
      vann:example "BipolarProject hasGeneticDataCollector JohnSmith"@en .

:doesNotHaveFamilyHistoryOf
      a       owl:ObjectProperty ;
      rdfs:comment "Describes which diseases the cohort group members do NOT have a family history of"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "does not family history of"@en ;
      rdfs:range :Disorder ;
      vann:example "CohortGroup1 doesNotHaveFamilyHistoryOf Schizophrenia --> CohortGroup1 excludes people who have a family history of schizophrenia"@en ;
      owl:propertyDisjointWith
              :hasFamilyHistoryOf .

terms:spatial
      a       owl:DatatypeProperty ;
      rdfs:isDefinedBy terms:spatial .

<http://vivoweb.org/ontology/core#College>
      a       owl:Class ;
      rdfs:comment "A college contributing to a given ENIGMA project"@en ;
      rdfs:isDefinedBy <http://vivoweb.org/ontology/core> ;
      rdfs:label "College"@en ;
      rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> ;
      vann:example "Los Angeles City College"@en .

:forOrganization
      a       owl:ObjectProperty ;
      rdfs:comment "Describes the organization associated with the qualified affiliation"@en ;
      rdfs:domain :QualifiedAffiliation ;
      rdfs:label "for organization"@en ;
      rdfs:range <http://xmlns.com/foaf/0.1/Organization> ;
      vann:example "JohnSmith hasQualifiedAffiliation \"QualifiedAffiliation\" forOrganization USC"@en .

:AcquisitionProtocol
      a       owl:Class ;
      rdfs:comment "The protocol used to acquire the scanner data types"@en ;
      rdfs:label "AcquisitionProtocol"@en ;
      vann:example "ENIGMA-Sulci"@en .

:hasProjectLead
      a       owl:ObjectProperty ;
      rdfs:comment "Describes what project someone is a project lead of"@en , "Describes who the leads of a project are"@en ;
      rdfs:domain :Project ;
      rdfs:label "has project lead"@en ;
      rdfs:range :Person ;
      rdfs:subPropertyOf terms:contributor ;
      vann:example "BipolarProject hasProjectLead JohnSmith"@en .

:GeneticAssociationProtocol
      a       owl:Class ;
      rdfs:comment "ENIGMA Protocols for genetic associations"@en ;
      rdfs:label "Genetic Association Protocol"@en ;
      vann:example "ENIGMA3 â€“ GWAS Meta Analysis of Cortical Thickness and Surface Area - Association Protocol"@en .

:usesTreatment
      a       owl:ObjectProperty ;
      rdfs:comment "Describes which medications or therapies that the members of the cohort group are using"@en ;
      rdfs:domain :CohortGroup ;
      rdfs:label "uses treatment"@en ;
      rdfs:range :Treatment ;
      vann:example "CohortGroup1 usesTreatment Antidepressants --> CohortGroup1 only includes people who currently use antidepressants"@en .

:DiagnosticCriteria
      a       owl:Class ;
      rdfs:comment "The principles and standards used to diagnose a diease or condition"@en ;
      rdfs:label "Diagnostic Criteria"@en ;
      vann:example "DSM4"@en .

:DataType
      a       owl:Class ;
      rdfs:comment "Specifies the kinds of data collected"@en ;
      rdfs:label "Data Type"@en , "DataType"@en .

:MedicalRating
      a       owl:Class ;
      rdfs:comment "Systems or scales used to measure the severity of a medical condition"@en ;
      rdfs:label "Medical Rating"@en ;
      vann:example "ClincalGlobalImpressionsScale"@en .

<http://xmlns.com/foaf/0.1/Organization>
      a       owl:Class ;
      rdfs:comment "An organization contributing to a given ENIGMA project"@en ;
      rdfs:isDefinedBy <http://xmlns.com/foaf/0.1> ;
      owl:equivalentClass :Organization .

:currentJobTitle
      a       owl:DatatypeProperty ;
      rdfs:comment "Data property that aims to capture the current job title of a person. Examples are student, faculty, staff, postdoc, etc."@en ;
      rdfs:domain :Person ;
      rdfs:label "current job title"@en ;
      rdfs:range xsd:string ;
      vann:example "JohnSmith currentJobTitle \"Postdoc\""@en .

:YoungManiaRatingScale
      a       owl:Class ;
      rdfs:comment "Rating scale used to measure the severity of manic episodes in children and young adults"@en ;
      rdfs:label "Young Mania Rating Scale"@en ;
      rdfs:subClassOf :MedicalRating .

:OrganicMedicalDisorder
      a       owl:Class ;
      rdfs:comment "Disorder characterized by decreased mental function due to a medical or physical disease"@en ;
      rdfs:label "Organic Medical Disorder"@en ;
      rdfs:subClassOf :Disorder ;
      vann:example "Delirium"@en .

:hasMiddleInitial
      a       owl:DatatypeProperty ;
      rdfs:comment "Describes middle initial of a person" ;
      rdfs:domain :Person ;
      rdfs:label "has middle initial"@en ;
      rdfs:range xsd:string ;
      vann:example "John A Smith hasMiddleInitial \"A\"" .

:shortName
      a       owl:DatatypeProperty ;
      rdfs:comment "A short name for the working group, in case the working group name is too long"@en ;
      rdfs:domain :WorkingGroup ;
      rdfs:label "short name"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf terms:title .

vann:preferredNamespacePrefix
      a       owl:AnnotationProperty .

terms:contributor
      a       owl:AnnotationProperty , owl:ObjectProperty ;
      rdfs:domain :Project ;
      rdfs:isDefinedBy terms:contributor ;
      rdfs:range :Person .

terms:identifier
      a       owl:DatatypeProperty ;
      rdfs:isDefinedBy terms:identifier .

:BrainScanDataType
      a       owl:Class ;
      rdfs:comment "Specifies the kinds of brain scan data collected"@en ;
      rdfs:label "Brain Scan Data Type"@en ;
      rdfs:subClassOf :DataType ;
