@prefix : <https://w3id.org/enigma#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/enigma> .

<https://w3id.org/enigma> rdf:type owl:Ontology ;
                           owl:versionIRI <https://w3id.org/enigma/pd-cardiovascular/1.1.0> ;
                           <http://purl.org/dc/terms/contributor> "Agnes McMahon" ,
                                                                  "MiHyun Jang" ,
                                                                  "Michael Bornstein" ,
                                                                  "Ryan Espiritu" ;
                           <http://purl.org/dc/terms/creator> "Daniel Garijo" ,
                                                              "Haripriya Dharmala" ,
                                                              "Joanna Bright" ,
                                                              "Josh Boyd" ,
                                                              "Kevin Low" ,
                                                              "Neda Jahanshad" ,
                                                              "Wesley Surento" ,
                                                              "Yolanda Gil" ;
                           <http://purl.org/dc/terms/description> "An ontology to describe cohorts of patients participating in ENIGMA projects."@en ;
                           <http://purl.org/dc/terms/license> <http://creativecommons.org/licenses/by/2.0/> ;
                           <http://purl.org/dc/terms/title> "The PD Cardiovascular Ontology"@en ;
                           <http://purl.org/vocab/vann/preferredNamespacePrefix> "enigma" ;
                           <http://purl.org/vocab/vann/preferredNamespaceUri> : ;
                           owl:versionInfo "v1.1.0" .

