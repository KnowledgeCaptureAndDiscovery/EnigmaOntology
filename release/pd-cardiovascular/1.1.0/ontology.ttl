###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/example
<http://purl.org/vocab/vann/example> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.linkedmodel.org/schema/vaem#rationale
<http://www.linkedmodel.org/schema/vaem#rationale> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://xmlns.com/foaf/0.1/homepage
<http://xmlns.com/foaf/0.1/homepage> rdf:type owl:ObjectProperty ;
                                     rdfs:comment "A homepage for some thing." ;
                                     rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> ;
                                     rdfs:label "homepage" .


###  https://schema.org/distribution
<https://schema.org/distribution> rdf:type owl:ObjectProperty ;
                                  rdfs:isDefinedBy <https://schema.org/> .


###  https://w3id.org/enigma#hasBrainScanDataType
:hasBrainScanDataType rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasDataType ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :Cohort
                                                  :CohortGroup
                                                  :CohortProject
                                                  :Project
                                                )
                                  ] ;
                      rdfs:range :BrainScanDataType ;
                      <http://purl.org/vocab/vann/example> "Cohort1 hasBrainScanDataType MRI"@en ;
                      rdfs:comment "Describes the brain scan data types that were collected for a cohort or project"@en ;
                      rdfs:label "has brain scan data type"@en .


###  https://w3id.org/enigma#hasCohortGroup
:hasCohortGroup rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Cohort
                                            :CohortProject
                                          )
                            ] ;
                rdfs:range :CohortGroup ;
                <http://purl.org/vocab/vann/example> "QTIM has cohort group QTIM_control"@en ;
                rdfs:comment "Property that indicates the cohort groups part of a cohort"@en ;
                rdfs:label "has cohort group"@en .


###  https://w3id.org/enigma#hasCohortProject
:hasCohortProject rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Cohort
                                              :CohortGroup
                                              :ImageAcquisitionProtocol
                                            )
                              ] ;
                  rdfs:range :CohortProject ;
                  <http://purl.org/vocab/vann/example> "BipolarCohort hasCohortProject BipolarBarcelonaCohort"@en ;
                  rdfs:comment "Describes the subcohorts, specific to a given project, that are part of that cohort"@en ;
                  rdfs:label "has cohort project"@en .


###  https://w3id.org/enigma#hasContact
:hasContact rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Cohort
                                        :CohortGroup
                                        :CohortProject
                                      )
                        ] ;
            rdfs:range :Person ;
            <http://purl.org/vocab/vann/example> "BipolarCohort hasContact JohnSmith"@en ;
            rdfs:comment "Property that points to the person that is a contact for the project or cohort"@en ;
            rdfs:label "has contact"@en .


###  https://w3id.org/enigma#hasControlGroup
:hasControlGroup rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasCohortGroup ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Cohort
                                             :CohortProject
                                           )
                             ] ;
                 rdfs:range :CohortGroup ;
                 <http://purl.org/vocab/vann/example> "BipolarCohort hasControlGroup BipolarControlGroup"@en ;
                 rdfs:comment "Describes the control group of a cohort"@en ;
                 rdfs:label "has control group"@en .


###  https://w3id.org/enigma#hasCovariates
:hasCovariates rdf:type owl:ObjectProperty ;
               rdfs:domain :Cohort ;
               rdfs:range :Covariates ;
               rdfs:label "has covariates" .


###  https://w3id.org/enigma#hasDataType
:hasDataType rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Cohort
                                         :Project
                                       )
                         ] ;
             rdfs:range :DataType ;
             <http://purl.org/vocab/vann/example> "Cohort1 hasDataType dataType1"@en ;
             rdfs:comment "Describes the data types that a cohort or project collected"@en ;
             rdfs:label "has data type"@en .


###  https://w3id.org/enigma#hasDataset
:hasDataset rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Cohort
                                        :CohortProject
                                      )
                        ] ;
            rdfs:range :Dataset ;
            rdfs:comment "Property that links a cohort project with the data being described."@en ;
            rdfs:label "has dataset"@en .


###  https://w3id.org/enigma#hasDiagnosticGroup
:hasDiagnosticGroup rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCohortGroup ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Cohort
                                                :CohortProject
                                              )
                                ] ;
                    rdfs:range :CohortGroup ;
                    <http://purl.org/vocab/vann/example> "BipolarCohort hasPatientGroup BipolarPatientGroup"@en ;
                    rdfs:comment "Describes the patient group of a cohort"@en ;
                    rdfs:label "has diagnostic group"@en .


###  https://w3id.org/enigma#hasDiagnosticInstrument
:hasDiagnosticInstrument rdf:type owl:ObjectProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( :CohortGroup
                                                     :CohortProject
                                                   )
                                     ] ;
                         rdfs:range :DiagnosticInstrument ;
                         <http://purl.org/vocab/vann/example> "Cohort1 hasDiagnosticInstrument miniInternationalNeuropychiatricInterview --> The members of Cohort1 are diagnosed with Mini International Neuropychiatric Interview"@en ;
                         rdfs:comment "Describes what instrument is used to diagnose the members of the cohort"@en ;
                         rdfs:label "has diagnostic instrument"@en .


###  https://w3id.org/enigma#hasDistribution
:hasDistribution rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <https://schema.org/distribution> ;
                 rdfs:domain :Dataset ;
                 rdfs:range :DataDownload ;
                 rdfs:comment "Indicates a downloadable form of a dataset at a specific location" ;
                 rdfs:label "has distribution" .


###  https://w3id.org/enigma#hasGeneticDataType
:hasGeneticDataType rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasDataType ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Cohort
                                                :CohortGroup
                                                :CohortProject
                                              )
                                ] ;
                    rdfs:range :GeneticDataType ;
                    <http://purl.org/vocab/vann/example> "Cohort1 hasGeneticDataType epigenetic"@en ;
                    rdfs:comment "Describes the genetic data types that were collected for a cohort or project"@en ;
                    rdfs:label "has genetic data type"@en .


###  https://w3id.org/enigma#hasHomePage
:hasHomePage rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Cohort
                                         :CohortGroup
                                         :CohortProject
                                         :Person
                                       )
                         ] ;
             rdfs:range owl:Thing .


###  https://w3id.org/enigma#hasImageAcquisitionProtocol
:hasImageAcquisitionProtocol rdf:type owl:ObjectProperty ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( :Cohort
                                                         :CohortGroup
                                                         :CohortProject
                                                       )
                                         ] ;
                             rdfs:range :ImageAcquisitionProtocol ;
                             <http://purl.org/vocab/vann/example> "Cohort1 hasImageAcquisitionProtocol AcquisitionProtocol1"@en ;
                             rdfs:comment "Describes the acquisition protocol that was used to scan the data of a cohort"@en ;
                             rdfs:label "has image acquisition protocol"@en .


###  https://w3id.org/enigma#hasPersonRole_Investigator
:hasPersonRole_Investigator rdf:type owl:ObjectProperty ;
                            rdfs:domain :Cohort ;
                            rdfs:range :Person ;
                            <http://purl.org/vocab/vann/example> "BipolarCohort hasInvestigator JohnSmith"@en ;
                            rdfs:comment "Describe the investigators of a cohort"@en ;
                            rdfs:label "has person role investigator"@en .


###  https://w3id.org/enigma#hasPersonRole_MainPrincipalInvestigator
:hasPersonRole_MainPrincipalInvestigator rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf :hasPersonRole_PrincipalInvestigator ;
                                         rdfs:domain [ rdf:type owl:Class ;
                                                       owl:unionOf ( :Cohort
                                                                     :CohortGroup
                                                                     :CohortProject
                                                                   )
                                                     ] ;
                                         rdfs:range :Person ;
                                         <http://purl.org/vocab/vann/example> "Cohort1 hasMainPrincipalInvestigatorof JohnSmith"@en ;
                                         rdfs:comment "Describes which cohorts somone is a main PI of"@en ;
                                         rdfs:label "has main person role principal investigator"@en .


###  https://w3id.org/enigma#hasPersonRole_PrincipalInvestigator
:hasPersonRole_PrincipalInvestigator rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf :hasPersonRole_Investigator ;
                                     rdfs:domain :Cohort ;
                                     rdfs:range :Person ;
                                     <http://purl.org/vocab/vann/example> "Cohort1 hasPrincipalInvestigator John --> the principal investigator of Cohort1 is John"@en ;
                                     rdfs:comment "Describes the principal investigator of the cohort"@en ;
                                     rdfs:label "has person role principal investigator"@en .


###  https://w3id.org/enigma#hasPopulationGroup
:hasPopulationGroup rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCohortGroup ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Cohort
                                                :CohortGroup
                                                :CohortProject
                                              )
                                ] ;
                    rdfs:range :CohortGroup ;
                    <http://purl.org/vocab/vann/example> "BipolarCohort hasPopulationGroup USCPopulationGroup"@en ;
                    rdfs:comment "Describes the population group of a population-based cohort"@en ;
                    rdfs:label "has population group"@en .


###  https://w3id.org/enigma#hasProjectResults
:hasProjectResults rdf:type owl:ObjectProperty ;
                   rdfs:domain :CohortProject ;
                   rdfs:range :DataDownload ;
                   rdfs:comment "Property to link project results to a Cohort Project" ;
                   rdfs:label "has project results" .


###  https://w3id.org/enigma#hasStatistics
:hasStatistics rdf:type owl:ObjectProperty ;
               rdfs:domain :CohortProject ;
               rdfs:range :Dataset ;
               rdfs:comment "Property used to link a cohort with the summary statistics that may have been  produced after applying a given protocol"@en ;
               rdfs:label "has statistics"@en .


###  https://w3id.org/enigma#has_cardiovascular
:has_cardiovascular rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :Cohort ;
                    rdfs:range :Cardiovascular ;
                    rdfs:comment "Describes cardiovascular covariate properties specific to the PD working group" ;
                    rdfs:label "has cardiovascular" .


###  https://w3id.org/enigma#has_neuropsychiatric
:has_neuropsychiatric rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :Cohort ;
                      rdfs:range :Neuropsychiatric ;
                      rdfs:label "has neuropsychiatric" .


###  https://w3id.org/enigma#isPartOfCohort
:isPartOfCohort rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Cohort
                                            :CohortGroup
                                            :CohortProject
                                          )
                            ] ;
                rdfs:range :Cohort ;
                rdfs:comment "Property that indicates the cohort groups and cohort projects which are part of a cohort"@en ;
                rdfs:label "is part of cohort"@en .


###  https://w3id.org/enigma#isPartOfCohortGroup
:isPartOfCohortGroup rdf:type owl:ObjectProperty ;
                     rdfs:domain :CohortProject ;
                     rdfs:range :CohortGroup ;
                     rdfs:comment "Property that indicates the projects which are part of a cohort group"@en ;
                     rdfs:label "is part of cohort group"@en .


###  https://w3id.org/enigma#isPartOfCohortProject
:isPartOfCohortProject rdf:type owl:ObjectProperty ;
                       rdfs:domain :CohortGroup ;
                       rdfs:range :CohortProject ;
                       rdfs:comment "Property that indicates the cohort groups which are part of a cohort project"@en ;
                       rdfs:label "is part of cohort project"@en .


###  https://w3id.org/enigma#isPartOfProject
:isPartOfProject rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :CohortGroup
                                             :CohortProject
                                           )
                             ] ;
                 rdfs:range :Project ;
                 rdfs:comment "Property that indicates the cohort groups specific to a project"@en ;
                 rdfs:label "is part of project"@en .


###  https://w3id.org/enigma#isPartOfWorkingGroup
:isPartOfWorkingGroup rdf:type owl:ObjectProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :Cohort
                                                  :CohortGroup
                                                  :CohortProject
                                                )
                                  ] ;
                      rdfs:range :WorkingGroup ;
                      rdfs:comment "Property that indicates the cohorts which are a part of a working group"@en ;
                      rdfs:label "is part of working group"@en .


###  https://w3id.org/enigma#studiesDisorder
:studiesDisorder rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Project
                                             :WorkingGroup
                                           )
                             ] ;
                 rdfs:range :Disorder ;
                 rdfs:comment "Property that indicates which project/working group studies which disorder"@en ;
                 rdfs:label "studies disorder"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/enigma#AlcoholConsumption
:AlcoholConsumption rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :Cardiovascular ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Include info on how this was measured" ;
                    rdfs:label "Alcohol Consumption" .


###  https://w3id.org/enigma#AtrialFibrillation
:AtrialFibrillation rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :Cardiovascular ;
                    rdfs:range xsd:boolean ;
                    rdfs:comment "Atrial Fibrillation: irregular (and often rapid) heart rate caused by poor blood flow that can cause blood clots, strokes, etc." ;
                    rdfs:label "Atrial Fibrillation" .


###  https://w3id.org/enigma#BodyMassIndex
:BodyMassIndex rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Cardiovascular ;
               rdfs:range xsd:boolean ;
               rdfs:label "Body Mass Index" .


###  https://w3id.org/enigma#CoagulationScreenPerformed
:CoagulationScreenPerformed rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf owl:topDataProperty ;
                            rdfs:domain :Cardiovascular ;
                            rdfs:range xsd:boolean ;
                            rdfs:label "Coagulation Screen Performed" .


###  https://w3id.org/enigma#Diabetes
:Diabetes rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Cardiovascular ;
          rdfs:range xsd:boolean ;
          rdfs:label "Diabetes" .


###  https://w3id.org/enigma#FamilyHistoryOfCVDisease
:FamilyHistoryOfCVDisease rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdfs:domain :Cardiovascular ;
                          rdfs:range xsd:boolean ;
                          rdfs:comment "CV: Cardiovascular Disease" ;
                          rdfs:label "Family History of CV Disease" .


###  https://w3id.org/enigma#HeartRate
:HeartRate rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :Cardiovascular ;
           rdfs:range xsd:boolean ;
           rdfs:label "Heart Rate" .


###  https://w3id.org/enigma#HistoryOfIschamicHeartDisease
:HistoryOfIschamicHeartDisease rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf owl:topDataProperty ;
                               rdfs:domain :Cardiovascular ;
                               rdfs:range xsd:boolean ;
                               rdfs:comment "Ischamic Heart Disease: aka Coronary Heart Disease, heart problems caused by narrowed arteries that supply blood to the heart" ;
                               rdfs:label "History of Ischamic Heart Disease" .


###  https://w3id.org/enigma#HistoryOfStrokeOrTransientIschaemicAttack
:HistoryOfStrokeOrTransientIschaemicAttack rdf:type owl:DatatypeProperty ;
                                           rdfs:subPropertyOf owl:topDataProperty ;
                                           rdfs:domain :Cardiovascular ;
                                           rdfs:range xsd:boolean ;
                                           rdfs:comment "Transient Ischaemic Attack: A \"mini stroke\" with minor stroke symptoms" ;
                                           rdfs:label "History of Stroke or Transient Ischaemic Attack" .


###  https://w3id.org/enigma#Hyperlipidaemia
:Hyperlipidaemia rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :Cardiovascular ;
                 rdfs:range xsd:boolean ;
                 rdfs:comment "Hyperlipidaemia: High levels of lipids/cholesterol/fats in the blood" ;
                 rdfs:label "Hyperlipidaemia" .


###  https://w3id.org/enigma#Hypertension
:Hypertension rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Cardiovascular ;
              rdfs:range xsd:boolean ;
              rdfs:comment "Hypertension: high blood pressure" ;
              rdfs:label "Hypertension" .


###  https://w3id.org/enigma#OrthostaticHypertension
:OrthostaticHypertension rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:domain :Cardiovascular ;
                         rdfs:range xsd:boolean ;
                         rdfs:comment "Orthostatic Hypertension: an increase in blood pressure upon assuming an upright position" ;
                         rdfs:label "Orthostatic Hypertension" .


###  https://w3id.org/enigma#OtherRelevantCardiovascularMedications
:OtherRelevantCardiovascularMedications rdf:type owl:DatatypeProperty ;
                                        rdfs:subPropertyOf owl:topDataProperty ;
                                        rdfs:domain :Cardiovascular ;
                                        rdfs:range xsd:string ;
                                        rdfs:comment "e.g. antihypertensives, cholesterol lowering drugs, aspirin, steriods" ;
                                        rdfs:label "Other Relevant Cardiovascular Medications" .


###  https://w3id.org/enigma#OtherRelevantDiagnoses
:OtherRelevantDiagnoses rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain :Cardiovascular ;
                        rdfs:range xsd:string ;
                        rdfs:comment "e.g. chronic kidney disease and stage, migraines, arthritis, lupus" ;
                        rdfs:label "Other Relevant Diagnoses" .


###  https://w3id.org/enigma#Smoking
:Smoking rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain :Cardiovascular ;
         rdfs:range xsd:boolean ;
         rdfs:label "Smoking" .


###  https://w3id.org/enigma#SmokingPackYears
:SmokingPackYears rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :Cardiovascular ;
                  rdfs:range xsd:string ;
                  rdfs:comment "A \"pack year\" is defined as twenty cigarettes smoked everyday for one year" ;
                  rdfs:label "Smoking Pack Years" .


###  https://w3id.org/enigma#StandingBloodPressure
:StandingBloodPressure rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain :Cardiovascular ;
                       rdfs:range xsd:boolean ;
                       rdfs:comment "Standing blood pressure after standing for 3 minutes" ;
                       rdfs:label "Standing Blood Pressure" .


###  https://w3id.org/enigma#SupineBloodPressure
:SupineBloodPressure rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain :Cardiovascular ;
                     rdfs:range xsd:boolean ;
                     rdfs:comment "Supine blood pressure after resting for at least 5 minutes" ;
                     rdfs:label "Supine Blood Pressure" .


#################################################################
#    Classes
#################################################################

###  http://schema.org/DataDownload
<http://schema.org/DataDownload> rdf:type owl:Class ;
                                 rdfs:isDefinedBy <http://schema.org> ;
                                 rdfs:label "Data Download" .


###  http://schema.org/Dataset
<http://schema.org/Dataset> rdf:type owl:Class ;
                            rdfs:comment "Dataset" ;
                            rdfs:isDefinedBy <http://schema.org> ;
                            rdfs:label "Dataset" .


###  https://w3id.org/enigma#AnxietyDisorder
:AnxietyDisorder rdf:type owl:Class ;
                 rdfs:subClassOf :MentalHealthDisorder ;
                 <http://purl.org/vocab/vann/example> "PTSD"@en ;
                 rdfs:comment "A mental health disorder defined by feelings of anxiety, fear, or worry"@en ;
                 rdfs:label "Anxiety Disorder"@en .


###  https://w3id.org/enigma#BrainScanDataType
:BrainScanDataType rdf:type owl:Class ;
                   rdfs:subClassOf :DataType ;
                   <http://purl.org/vocab/vann/example> "EEG"@en ;
                   rdfs:comment "Specifies the kinds of brain scan data collected"@en ;
                   rdfs:label "Brain Scan Data Type"@en .


###  https://w3id.org/enigma#Cancer
:Cancer rdf:type owl:Class ;
        rdfs:subClassOf :Disorder ;
        <http://purl.org/vocab/vann/example> "CNS Cancer"@en ;
        rdfs:comment "A disease characterized by abnormal cell division and growth"@en ;
        rdfs:label "Cancer"@en .


###  https://w3id.org/enigma#Cardiovascular
:Cardiovascular rdf:type owl:Class ;
                rdfs:comment "Cardiovascular Covariates measured by each PD cohort" ;
                rdfs:label "Cardiovascular" .


###  https://w3id.org/enigma#CardiovascularDisorder
:CardiovascularDisorder rdf:type owl:Class ;
